{
 "languageserver": {
    "ccls": {
       "command": "/home/nhartung/dev/ccls/Release/ccls",
       "filetypes": ["c", "cpp", "objc", "objcpp"],
       "rootPatterns": [".ccls", "compile_commands.json", ".git/", ".hg/", ".svn/" ],
       "initializationOptions": {
          "cache": {
             "directory": "/tmp/ccls"
          }
       }
    },
    "dart": {
      "command": "/usr/bin/dart",
      "args": ["/usr/lib/dart/bin/snapshots/analysis_server.dart.snapshot", "--lsp"],
      "filetypes": ["dart"],
      "rootPatterns": ["pubspec.yaml"],
      "initializationOptions": {}
     }
  },
  "python.jediEnabled": false
}

